#include <iostream>
using namespace std ;
template <class t>
class stack{
private :
    t top , arr[5] ;

public:
    stack() // stack() : top(value of top) {}
    {
        top = -1 ;
    }
    void push(t Element)
    {
        if(top == 4)
            cout << "stack is full\n" ;
        else
        arr[++top] = Element ;
    }
    bool isEmpty()
    {
        return (top == -1 ? 1 : 0) ;
    }
    void pop()
    {
        if(isEmpty())
            cout << "stack is empty\n" ;
        else
        arr[top--] = 0 ;
    }
    void pop(t& Element)
    {
        if(isEmpty())
            cout << "stack is empty\n" ;
        else
        {
            Element = arr[top--] ;
        }

    }
    void getTop(t& Element)
    {
        if(isEmpty())
            cout << "stack is empty\n" ;
        else
        {
            Element = arr[top] ;
        }

    }
    void Display()
    {
        for(int i = top ; i >= 0  ; i--)
            cout << arr[i] << "\n" ;
    }
};



int main()
{
stack<char> s ;
s.push('f') ;
char x = 0 ;
s.pop(x) ;
cout << x ;
    return 0 ;
}
